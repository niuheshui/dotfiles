[Volume]
command=echo 󰕾 $(amixer -c 1 get Master | grep -o -E '([0-9]*%|\[(off|on)\])' | sort | uniq)
color=#FF00FF
interval=once
signal=1

[Brightness]
command=python3 -c "print(f'󰖨 {int($(brightnessctl g) / $(brightnessctl m) * 100)}')"
color=#FF0000
interval=once
signal=2

[Time]
#command=date +%H:%M:%S
command=date 
color=#FFFF00
interval=1
